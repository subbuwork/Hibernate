<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
 <hibernate-mapping>
<class name="com.sparing.hibernate.entity.Userxml" table="User">
<id name="uid" type="int" column="uid">
<generator class="assigned"></generator>
</id> 
 <property name="userName" column="u_name" type="string"></property>
 <property name="address" column="u_address" type="string"></property>
 </class> 
 </hibernate-mapping>